"jest": {
  "preset": "jest-preset-angular",
  "setupFilesAfterEnv": [
    "<rootDir>/setup-jest.ts"
  ],
  "roots": [
    "<rootDir>/src"
  ],
  "testMatch": [
    "**/+(*.)+(spec|test).+(ts|js)?(x)"
  ],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "lcov",
    "text",
    "cobertura"
  ],
  "collectCoverageFrom": [
    "src/**/*.ts",
    "!src/**/*.spec.ts",
    "!src/**/*.module.ts",
    "!src/main.ts",
    "!src/polyfills.ts",
    "!src/environments/**"
  ],
  "transformIgnorePatterns": [
    "node_modules/(?!.*\\.mjs$|ag-grid-angular|ag-grid-community)"
  ]
}